TRUE   <-- chem: chemistry included (true)
TRUE   <-- mic: microphysics included (true)
FALSE  <-- rst: restart (true) or new initialization (false) of model run
TRUE   <-- netCDF: netCDF output 
FALSE  <-- binout: binary output
FALSE  <-- box: run model as box version (true)
TRUE   <-- halo: halogen chemistry included (true)
FALSE  <-- iod: iodine chemistry included (true) (automatically off, when 'halo = false')
FALSE   <-- nuc: nucleation included (true)
3      <-- iaertyp: aerosoltype 1=urban; 2=rural; 3=maritime
96     <-- lstmax: integration time [hours]
300    <-- scaleo3_m: O3 [Dobson units] (only for scaling in photolysis calculations)
4      <-- nkc_l: number of output classes for liquid chem (set to <nkc if no cloud expected)
1      <-- neula: eulerian (=0, includes advection of chem species) or lagrangian (=1, no advection)
